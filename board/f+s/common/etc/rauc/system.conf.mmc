[system]
compatible=rauc-demo-mx8mm
bootloader=uboot
mountprefix=/mnt/rauc

[keyring]
path=rauc.cert.pem
use-bundle-signing-time=true

[slot.rootfs.0]
device=/dev/mmcblk2p5
type=raw
parent=boot.0

[slot.rootfs.1]
device=/dev/mmcblk2p6
type=raw
parent=boot.1

[slot.boot.0]
device=/dev/mmcblk2p1
type=vfat
bootname=A

[slot.boot.1]
device=/dev/mmcblk2p2
type=vfat
bootname=B
